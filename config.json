{
    "apis": {
        "esv": {
            "baseURL": "https://api.esv.org/v3/",
            "methods": {
                "getPassage": {
                    "url": "/passage/text",
                    "method": "get",
                    "repl": true,
                    "params": {
                        "include-headings": true,
                        "include-footnotes": false,
                        "include-verse-numbers": true,
                        "include-short-copyright": false,
                        "include-passage-references": false
                    },
                    "messages": {
                        "404": "Error: Passage not found"
                    },
                    "apiKey": true
                }
            }
        }
    }
}